{
  "manifest_version": 2,
  "name": "Forager",
  "description": "Hunt and gather data",
  "version": "3.0.0",
  "version_name": "3.0.0 beta.1",
  "permissions": [
    "activeTab",
    "storage",
    "http://localhost/*"
  ],
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "img/icon48.png"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    "img/home-icon.png"
  ]
}
