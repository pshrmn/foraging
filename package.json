  "license": "MIT",
  "author": "Paul Sherman",
  "repository": {
    "type": "git",
    "url": "git://github.com/pshrmn/forager.git"
  },
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" forager/css/interface.css --dir forager/css/",
    "css": "npm run sass & npm run autoprefix",
    "build:dev": "npm run webpack:dev & npm run css",
    "build:prod": "npm run webpack:prod & npm run css",
    "sass": "node-sass scss/** -o forager/css/**",
    "test": "cross-env NODE_PATH=src mocha tests --compilers js:babel-core/register --recursive",
    "webpack:dev": "webpack",
    "webpack:prod": "webpack",
    "webpack:watch": "webpack --watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.4.1",
    "cross-env": "^2.0.1",
    "jsdom": "^8.3.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.10.1",
    "postcss-cli": "^2.3.2",
    "redux-mock-store": "^1.0.3",
    "sinon": "^1.17.4",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "d3-hierarchy": "^1.0.2",
    "d3-path": "^1.0.1",
    "expiring-redux-messages": "^0.3.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "simple-wizard-component": "^0.5.1"
  }
}
