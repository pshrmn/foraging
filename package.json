{
  "name": "Forager",
  "version": "2.33.1",
  "license": "MIT",
  "author": "Paul Sherman",
  "repository": {
    "type": "git",
    "url": "git://github.com/pshrmn/forager.git"
  },
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" forager/css/interface.css --dir forager/css/",
    "css": "npm run sass & npm run autoprefix",
    "build:dev": "npm run webpack:dev & npm run css",
    "build:prod": "npm run webpack:prod & npm run css",
    "sass": "node-sass scss/** -o forager/css/**",
    "test": "cross-env NODE_PATH=src jest",
    "test:watch": "npm run test -- --watch",
    "coverage": "npm run test -- --coverage",
    "webpack:dev": "webpack",
    "webpack:prod": "cross-env BABEL_ENV=production webpack",
    "webpack:watch": "webpack --watch"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "jest": "^16.0.2",
    "node-sass": "^4.0.0",
    "postcss-cli": "^2.6.0",
    "react-test-renderer": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "d3-hierarchy": "^1.0.3",
    "d3-path": "^1.0.3",
    "expiring-redux-messages": "^0.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "simple-wizard-component": "^0.6.1"
  },
  "jest": {
    "modulePaths": [
      "/src"
    ]
  }
}
