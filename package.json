{
  "name": "Forager",
  "version": "2.14.9",
  "license": "MIT",
  "author": "Paul Sherman",
  "repository": {
    "type": "git",
    "url": "git://github.com/pshrmn/forager.git"
  },
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" forager/css/interface.css --dir forager/css/",
    "css": "npm run sass & npm run autoprefix",
    "build-dev": "npm run webpack-dev & npm run css",
    "build-prod": "npm run webpack-prod & npm run css",
    "sass": "node-sass scss/** -o forager/css/**",
    "test": "mocha tests --harmony_array_includes --compilers js:babel-core/register --recursive",
    "webpack-dev": "webpack && rm -f forager/bundle.js.map",
    "webpack-prod": "webpack --config=webpack.config.min.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.4.1",
    "jsdom": "^8.3.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.3.2",
    "redux-mock-store": "^1.0.2",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "d3": "^3.5.16",
    "expiring-redux-messages": "^0.3.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "simple-wizard-component": "^0.3.1"
  }
}
