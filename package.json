{
  "name": "Forager",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Paul Sherman",
  "repository": {
    "type": "git",
    "url": "git://github.com/psherman/forager.git"
  },
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions\" forager/css/interface.css --dir forager/css/",
    "build": "npm run webpack & npm run css",
    "css": "npm run sass & npm run autoprefix",
    "sass": "node-sass scss/** -o forager/css/**",
    "test": "mocha specs --require specs/helpers/chai.js --compilers js:babel/register",
    "webpack": "webpack & webpack --config=webpack.config.min.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.20",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.3.2",
    "webpack": "^1.12.4"
  },
  "dependencies": {
    "d3": "^3.5.8",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "redux": "^3.0.4"
  }
}
